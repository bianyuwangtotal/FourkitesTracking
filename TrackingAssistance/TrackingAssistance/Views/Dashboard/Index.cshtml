

@model IEnumerable<TrackingAssistance.Models.LoadDisplay>
<div class="content" id="tableBody">
    @Html.Partial("_TablePartialView", Model)
</div>
<link href="~/Content/jquery.pnotify.css" rel="stylesheet" />

<div class="modal fade" id="actionModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <!--head of login modal-->
            <div class="modal-header" style=" background-color:#333; color:white">

                <input type="hidden" id="blockAction" />
                <input type="hidden" id="actionDone" />
                <h3 class="modal-title">
                    Action Comfirm:
                </h3>
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    ×
                </button>
            </div>
            <div id="message" style="padding: 25px;">
            </div>
            <div class="modal-footer">
                <a href="#" class="btn btn-default" data-dismiss="modal">Cancel</a>
                <button class="btn btn-success" onclick="updatecheck()">Confirm</button>
            </div>
        </div>
    </div>
</div>
